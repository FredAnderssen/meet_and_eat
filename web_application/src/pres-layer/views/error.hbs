{{#if model}}


        <section class="hero is-danger">
                <div class="hero-body">
                        <div class="container">
                                <h1 class="title">
                                        Error!
                                </h1>
                                <h2 class="subtitle">
                                        <ul>
                                                {{#each model}}
                                                        <li>* {{this.this}}</li>
                                                {{/each}}
                                        </ul>
                                </h2>
                        </div>
                </div>
        </section>
{{/if}}